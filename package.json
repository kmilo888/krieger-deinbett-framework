{
  "name": "krieger-deinbett-framework",
  "version": "1.0.0",
  "description": "An e2e framework for testing Deinbett.de page",
  "main": "index.js",
  "scripts": {
    "cypress:ui": "cypress open",
    "cypress:run": "npx cypress run",
    "cypress:reports": "npm run cypress:run && node cypress/reports/cucumber-html-reports",
    "docker:build": "docker build -t krieger-deinbett-framework-image:1.0.0 .",
    "docker:run": "docker run -i -v \"%cd%\":///krieger-deinbett-framework -t krieger-deinbett-framework-image:1.0.0"    
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kmilo888/krieger-deinbett-framework.git"
  },
  "keywords": [
    "javascript",
    "cypress",
    "cucumber",
    "pom"
  ],
  "author": "Camilo Parra",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kmilo888/krieger-deinbett-framework/issues"
  },
  "homepage": "https://github.com/kmilo888/krieger-deinbett-framework#readme",
  "devDependencies": {
    "cypress": "^9.7.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "multiple-cucumber-html-reporter": "^1.21.4"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/reports/json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
